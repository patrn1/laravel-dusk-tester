{
  "name": "tarampampam/laravel-dusk-tester",
  "description": "Package for running dusk tests with Laravel",
  "keywords": ["laravel", "dusk"],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Tarampampam",
      "email": "tarampampam@users.noreply.github.com",
      "homepage": "https://github.com/tarampampam/"
    }
  ],
  "require": {
    "php": ">=7.0",
    "laravel/framework": "5.4.*",
    "laravel/dusk": "^1.1",
    "phpunit/phpunit": "^5.7"
  },
  "require-dev": {},
  "autoload": {
    "psr-4": {
      "Tarampampam\\LaravelDuskTester\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tarampampam\\LaravelDuskTester\\Tests\\": "tests"
    }
  },
  "suggest": {
    "guzzlehttp/guzzle": "For making request to the external resources in unit tests"
  },
  "support": {
    "issues": "https://github.com/tarampampam/laravel-dusk-tester/issues",
    "source": "https://github.com/tarampampam/laravel-dusk-tester"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Tarampampam\\LaravelDuskTester\\LaravelDuskTesterServiceProvider"
      ]
    }
  }
}